"      {{#if status }}\n"
"        <p><strong>Status</strong>: {{ status }} {{#if updated }} (last "
"updated {{ updated }}) {{/if }}</p>\n"
"      {{/if }}\n"
"\n"
"      <p><strong>Cost</strong>: {{ cost }}</p>\n"
"      {{#if agency}}<p><strong>Agency</strong>: {{ agency }}</p> {{/if}}\n"
